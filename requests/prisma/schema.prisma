// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Customer {
  id          String @id @default(uuid())
  externalId  String
  email       String @unique

  @@map("customers")
}

model Product {
  id          String @id @default(uuid())
  externalId  String
  code        String

  @@map("products")
}